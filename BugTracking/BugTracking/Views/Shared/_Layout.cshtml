    <script>
    $(document).ready(function(){

        $('.summernote').summernote();

    });

    $('#data_1 .input-group.date').datepicker({
        todayBtn: "linked",
        keyboardNavigation: false,
        forceParse: false,
        calendarWeeks: true,
        autoclose: true
       
      
    });
    </script>

